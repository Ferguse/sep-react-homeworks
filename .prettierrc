proseWrap: always
printWidth: 80
tabWidth: 2
singleQuote: true
trailingComma: all
bracketSpacing: true
jsxBracketSameLine: false
semi: true
useTabs: false
arrowParens: avoid
parser: babylon
overrides:
- files:
  - '*.js'
  - '*.jsx'
  options:
    parser: typescript
- files: '*.md'
  options:
    parser: markdown
- files: '*.json'
  options:
    parser: json
    printWidth: 80
- files: '*.pcss'
  options:
    parser: css